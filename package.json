{
  "name": "aerie-ui",
  "description": "The client application for Aerie.",
  "version": "0.12.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NASA-AMMOS/aerie-ui.git"
  },
  "engines": {
    "node": ">=16.14.0",
    "npm": ">=8.3.1"
  },
  "scripts": {
    "build": "rm -rf build && svelte-kit build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "cloc": "cloc src",
    "dev": "npm run version && svelte-kit dev",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "pre-commit": "node ./scripts/pre-commit.js",
    "prebuild": "npm run version",
    "preview": "svelte-kit preview",
    "test:e2e": "playwright test",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:e2e:dev": "AUTH_TYPE=none ORIGIN=http://localhost:3000 node build",
    "test:unit": "vitest run",
    "version": "node ./scripts/version.js"
  },
  "devDependencies": {
    "@nasa-jpl/stellar": "^1.0.3",
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-node": "1.0.0-next.78",
    "@sveltejs/kit": "1.0.0-next.350",
    "@testing-library/svelte": "^3.1.3",
    "@types/cookie": "^0.5.1",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-brush": "^3.0.1",
    "@types/d3-drag": "^3.0.1",
    "@types/d3-quadtree": "^3.0.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-selection": "^3.0.2",
    "@types/d3-shape": "^3.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/toastify-js": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "bootstrap-icons": "^1.8.3",
    "cloc": "^2.9.0",
    "cookie": "^0.5.0",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-drag": "^3.0.0",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql-ws": "^5.9.0",
    "jsdom": "^20.0.0",
    "lodash-es": "^4.17.21",
    "modern-css-reset": "^1.4.0",
    "monaco-editor": "^0.33.0",
    "postgres-interval": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "split-grid": "^1.0.11",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-dnd-action": "^0.9.18",
    "svelte-preprocess": "^4.10.7",
    "tippy.js": "^6.3.7",
    "toastify-js": "^1.11.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unique-names-generator": "^4.7.1",
    "vitest": "^0.15.2",
    "vitest-svelte-kit": "^0.0.6"
  }
}
