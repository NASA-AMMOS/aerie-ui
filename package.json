{
  "name": "aerie-ui",
  "description": "The client application for Aerie.",
  "version": "0.13.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NASA-AMMOS/aerie-ui.git"
  },
  "engines": {
    "node": ">=16.17.0",
    "npm": ">=8.15.0"
  },
  "scripts": {
    "build": "rm -rf build && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cloc": "cloc src",
    "dev": "npm run version && vite dev --port 3000",
    "format:check": "prettier --check --plugin-search-dir=. .",
    "format:write": "prettier --write --plugin-search-dir=. .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:css": "stylelint **/*.{css,svelte}",
    "lint:css:fix": "stylelint **/*.{css,svelte} --fix",
    "prebuild": "npm run version",
    "predev": "node ./scripts/check-node.cjs",
    "preview": "vite preview --port 3000",
    "sync": "svelte-kit sync",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:unit": "vitest run",
    "version": "node ./scripts/version.js"
  },
  "dependencies": {
    "@nasa-jpl/stellar": "^1.0.10",
    "@sveltejs/adapter-node": "1.0.0-next.96",
    "@sveltejs/kit": "1.0.0-next.504",
    "ag-grid-community": "^28.1.1",
    "bootstrap": "^5.2.1",
    "bootstrap-icons": "^1.9.1",
    "cookie": "^0.5.0",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-drag": "^3.0.0",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.1.0",
    "graphql-ws": "^5.11.2",
    "lodash-es": "^4.17.21",
    "modern-css-reset": "^1.4.0",
    "monaco-editor": "^0.34.0",
    "postgres-interval": "^4.0.0",
    "split-grid": "^1.0.11",
    "svelte": "^3.50.1",
    "svelte-dnd-action": "^0.9.20",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte-tags-input": "2.9.2",
    "tippy.js": "^6.3.7",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.0",
    "@poppanator/sveltekit-svg": "^1.0.0",
    "@testing-library/svelte": "^3.2.1",
    "@types/cookie": "^0.5.1",
    "@types/d3-axis": "^3.0.1",
    "@types/d3-brush": "^3.0.1",
    "@types/d3-drag": "^3.0.1",
    "@types/d3-quadtree": "^3.0.2",
    "@types/d3-scale": "^4.0.2",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-selection": "^3.0.3",
    "@types/d3-shape": "^3.1.0",
    "@types/lodash-es": "^4.17.6",
    "@types/toastify-js": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "cloc": "^2.10.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^20.0.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "semver": "^7.3.7",
    "stylelint": "^14.12.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-order": "^5.0.0",
    "svelte-check": "^2.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "unique-names-generator": "^4.7.1",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  }
}
