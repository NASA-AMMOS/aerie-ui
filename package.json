{
  "name": "aerie-ui",
  "description": "The client application for Aerie.",
  "version": "1.1.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NASA-AMMOS/aerie-ui.git"
  },
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=8.19.0"
  },
  "scripts": {
    "build": "rm -rf build && vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "cloc": "cloc src",
    "dev": "npm run version && vite dev --port 3000",
    "format:check": "prettier --check --plugin-search-dir=. .",
    "format:write": "prettier --write --plugin-search-dir=. .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:css": "stylelint **/*.{css,svelte}",
    "lint:css:fix": "stylelint **/*.{css,svelte} --fix",
    "prebuild": "npm run version",
    "predev": "node ./scripts/check-node.cjs",
    "preview": "vite preview --port 3000",
    "sync": "svelte-kit sync",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:codegen": "playwright codegen http://localhost:3000",
    "test:e2e:debug": "PWDEBUG=1 playwright test",
    "test:unit": "vitest run",
    "version": "node ./scripts/version.js"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^4.5.12",
    "@nasa-jpl/stellar": "^1.0.26",
    "@sveltejs/adapter-node": "1.1.3",
    "@sveltejs/kit": "1.1.1",
    "ag-grid-community": "^29.1.0",
    "ajv": "^8.12.0",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "cookie": "^0.5.0",
    "d3-array": "^3.2.2",
    "d3-axis": "^3.0.0",
    "d3-brush": "^3.0.0",
    "d3-drag": "^3.0.0",
    "d3-quadtree": "^3.0.1",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "graphql-ws": "^5.11.3",
    "lodash-es": "^4.17.21",
    "modern-css-reset": "^1.4.0",
    "monaco-editor": "^0.36.0",
    "postgres-interval": "^4.0.0",
    "split-grid": "^1.0.11",
    "svelte": "^3.55.1",
    "svelte-dnd-action": "^0.9.22",
    "svelte-popperjs": "^1.3.2",
    "svelte-preprocess": "^5.0.1",
    "svelte-tags-input": "2.9.2",
    "tippy.js": "^6.3.7",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.1",
    "@poppanator/sveltekit-svg": "^2.1.2",
    "@testing-library/svelte": "^3.2.2",
    "@types/cookie": "^0.5.1",
    "@types/d3-array": "^3.0.4",
    "@types/d3-axis": "^3.0.2",
    "@types/d3-brush": "^3.0.2",
    "@types/d3-drag": "^3.0.2",
    "@types/d3-quadtree": "^3.0.2",
    "@types/d3-scale": "^4.0.3",
    "@types/d3-scale-chromatic": "^3.0.0",
    "@types/d3-selection": "^3.0.4",
    "@types/d3-shape": "^3.1.1",
    "@types/d3-time": "^3.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/toastify-js": "^1.11.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitest/ui": "^0.29.1",
    "cloc": "^2.11.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^21.1.0",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "semver": "^7.3.8",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-order": "^6.0.2",
    "svelte-check": "^3.0.4",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "unique-names-generator": "^4.7.1",
    "vite": "4.0.4",
    "vitest": "0.27.2"
  }
}
